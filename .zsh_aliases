alias emacs='emacs -nw'
alias ohmyzshrc='emacs -nw ~/.oh-my-zshrc'
alias zshrc='emacs -nw ~/.zshrc'
alias zshaliases='emacs -nw ~/.zsh_aliases'
alias tmuxconf='emacs ~/.tmux.conf'
alias ls='ls -a'
alias rezsh='source ~/.zshrc'

alias gs='git status'
alias ga='git add'
alias gc='git commit -v'
alias gl='git log'
alias gl1='git log --oneline'
alias gaa='git add --all'
alias gapa='git add --patch'
alias gclean_local='git branch -r | awk '"'"'{print $1}'"'"' | egrep -v -f /dev/fd/0 <(git branch -vv | grep origin) | awk '"'"'{print $1}'"'"' | xargs git branch -D'